# Environment configuration for Smart Traffic Backend
# Compatible package versions for different Python environments

# For Python 3.12 (latest)
TENSORFLOW_VERSION_PY312=2.19.0
NUMPY_VERSION_PY312=2.1.3
KERAS_VERSION_PY312=3.10.0

# For Python 3.8-3.11 (stable/fallback)
TENSORFLOW_VERSION_STABLE=2.15.0
NUMPY_VERSION_STABLE=1.25.2
KERAS_VERSION_STABLE=2.15.0

# Package index mirrors (in case of PyPI issues)
PIP_INDEX_URL=https://pypi.org/simple/
PIP_EXTRA_INDEX_URL=https://pypi.python.org/simple/

# Runtime settings
CUDA_VISIBLE_DEVICES=-1
TF_CPP_MIN_LOG_LEVEL=2